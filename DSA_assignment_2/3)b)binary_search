#include <iostream>
using namespace std;
int main() {
    int n;
    cin >> n;
    int a[100];
    for (int i = 0; i < n-1; i++) cin >> a[i];
    int l = 0, r = n-2, ans = n;
    while (l <= r) {
        int mid = (l+r)/2;
        if (a[mid] == mid+1) l = mid+1;
        else { ans = mid+1; r = mid-1; }
    }
    cout << ans;
}

